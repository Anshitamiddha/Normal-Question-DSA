#include<iostream>
using namespace std;
int main ()
{
   int x,y,z;
   cout<<"ENter first side"<<x<<endl;
   cin>>x;
   cout<<"ENter second side"<<y<<endl;
   cin>>y;
   cout<<"ENter third side"<<z<<endl;
   cin>>z;
   if(x==z && x==y)
      {
         cout<<"Equilateral triangle"<<endl;
      }
      else if(x==y || x==z || y==z)
      {
         cout<<"Isosceles triangle"<<endl;
      }
   
   else
   {
   cout<<"Scalene triangle"<<endl;}
   return 0;
}
