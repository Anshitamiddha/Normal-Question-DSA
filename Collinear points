#include<iostream>
using namespace std;
int main()
{
    int x1,x2,y1,y2,x3,y3,m,n;
    cout<<"Enter the first coordinates"<<endl;
    cin>>(x1,y1);
    cout<<"Enter the second coordinates"<<endl;
    cin>>(x2,y2);
    cout<<"Enter the third coordinates"<<endl;
    cin>>(x3,y3);
    m=(y2-y1)/(x2-x1);
    n=(y3-y2)/(x3-x2);
    if(m==n)
    {
        cout<<"Collinear"<<endl;
    }
    else
    {
        cout<<" Not collinear"<<endl;
    }
    return 0;
   
}
